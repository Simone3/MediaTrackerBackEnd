{
  "name": "media-tracker-back-end",
  "version": "1.0.0",
  "description": "Back-End for the Media Tracker app",
  "main": "index.ts",
  "scripts": {
    "test": "nyc ts-mocha --paths --file test/global/global-init-tests.ts -p test/tsconfig.json test/**/*.ts",
    "start": "ts-node-dev -r tsconfig-paths/register index.ts",
    "debug": "ts-node-dev -r tsconfig-paths/register --inspect --respawn index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simone3/MediaTrackerBackEnd.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Simone3/MediaTrackerBackEnd/issues"
  },
  "homepage": "https://github.com/Simone3/MediaTrackerBackEnd#readme",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "**/*.js.map"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "mocha": {
	  "timeout": 10000
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/cls-hooked": "^4.3.0",
    "@types/exit-hook": "^2.2.1",
    "@types/express": "^4.17.2",
    "@types/express-mung": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.12",
    "@types/mongoose": "^5.5.33",
    "@types/nock": "^11.1.0",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^18.4.3",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.35",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "^0.11.0",
    "exit-hook": "^2.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.3",
    "express-mung": "^0.5.1",
    "log4js": "^6.1.0",
    "mongoose": "^5.8.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "uuid": "^3.3.3"
  }
}
