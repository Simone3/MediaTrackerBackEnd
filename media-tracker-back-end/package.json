    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "nyc": "^14.1.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.35",
